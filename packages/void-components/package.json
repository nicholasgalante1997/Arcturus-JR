{
  "name": "@arcjr/void-components",
  "version": "1.0.0",
  "description": "React component library for the Void design system",
  "type": "module",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./void-react.css": "./dist/index.css",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "run-s bundle check-types",
    "bundle": "bun run build.ts",
    "bundle:watch": "bun run build.ts --watch",
    "check-types": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "clean": "rm -rf dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "bun test"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@arcjr/void-tokens": "workspace:*",
    "@arcjr/void-css": "workspace:*"
  },
  "dependencies": {
    "clsx": "^2.1.0"
  },
  "devDependencies": {
    "@arcjr/void-css": "workspace:*",
    "@arcjr/void-tokens": "workspace:*",
    "@storybook/addon-docs": "^10.0.7",
    "@storybook/react-vite": "^10.0.7",
    "@testing-library/react": "^16.0.0",
    "@types/bun": "latest",
    "@types/react": "^19.0.0",
    "happy-dom": "^20.0.10",
    "npm-run-all": "4.1.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "^10.0.7",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.0.0",
    "vite": "^7.2.2"
  },
  "keywords": [
    "react",
    "components",
    "void-design-system",
    "ui-library"
  ],
  "author": "Nick Galante",
  "license": "MIT"
}
