{
  "name": "@arcjr/void-css",
  "version": "1.0.0-rc.1",
  "private": true,
  "type": "module",
  "files": [
    "css",
    "dist",
    "postcss.config.mjs",
    "README.md",
    "package.json"
  ],
  "exports": {
    "./": "",
    "./css/*": "./css/*",
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "bun run postcss:void",
    "postcss:void": "postcss ./css --dir dist/css --ext .min.css",
    "serve:css": "bunx http-server ./css -p 8080 --open"
  },
  "peerDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "autoprefixer": "^10.4.23",
    "cssnano": "^7.1.2",
    "http-server": "^14.1.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^4.1.18"
  }
}
