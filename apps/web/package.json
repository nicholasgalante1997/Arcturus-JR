{
  "name": "@arcjr/web",
  "version": "1.0.0",
  "description": "A simple markdown web site for my (probably wrong) tech blog/opinions.",
  "author": "Nick Galante <rustycloud42@protonmail.com> (https://github.com/nicholasgalante1997)",
  "type": "module",
  "bin": {
    "ngx": "bin/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s bundle copy:ciphers copy:workers prerender postcss copy",
    "build-storybook": "storybook build",
    "bundle": "webpack --config webpack/prod.mjs",
    "bundle:analyze": "webpack-bundle-analyzer dist/stats.json",
    "cipher": "bun ./.cipher/index.ts",
    "clean": "run-p clean:css clean:dist",
    "clean:ciphers": "rm -rf public/ciphertexts dist/ciphertexts",
    "clean:css": "rm -f public/css/*.min.css && rm -f public/css/themes/*.min.css",
    "clean:dist": "rm -rf dist/",
    "clean:void-css": "rm -rf public/css/void",
    "copy": "run-p copy:assets copy:docs copy:favicons copy:markdown copy:styles",
    "copy:assets": "cp -R public/assets dist/",
    "copy:ciphers": "run-s copy:ciphers:public",
    "copy:ciphers:private": "run-s copy:ciphers:private:mkdir copy:ciphers:private:texts copy:ciphers:private:json",
    "copy:ciphers:private:json": "cp .cipher/ciphers.json public/ciphertexts/ciphers.json",
    "copy:ciphers:private:mkdir": "mkdir -p public/ciphertexts",
    "copy:ciphers:private:texts": "cp -R .cipher/ciphertexts/* public/ciphertexts/",
    "copy:ciphers:public": "run-s copy:ciphers:public:mkdir copy:ciphers:public:texts copy:ciphers:public:json",
    "copy:ciphers:public:json": "cp public/ciphertexts/ciphers.json dist/ciphertexts/ciphers.json",
    "copy:ciphers:public:mkdir": "mkdir -p dist/ciphertexts",
    "copy:ciphers:public:texts": "cp -R public/ciphertexts/* dist/ciphertexts/",
    "copy:docs": "cp -R public/docs dist/",
    "copy:favicons": "cp public/*.png public/*.ico public/site.webmanifest dist/",
    "copy:markdown": "cp -R public/content dist/",
    "copy:styles": "cp -R public/css dist/",
    "copy:void-css": "run-s copy:void-css:mkdir copy:void-css:tokens copy:void-css:styles copy:void-css:components",
    "copy:void-css:components": "cp node_modules/@arcjr/void-components/dist/index.css ./public/css/void/void-components.css",
    "copy:void-css:mkdir": "mkdir -p public/css/void/void-vanilla",
    "copy:void-css:styles": "cp -R node_modules/@arcjr/void-css/css/ public/css/void/void-vanilla",
    "copy:void-css:tokens": "cp node_modules/@arcjr/void-tokens/dist/css/void-tokens.css ./public/css/void/void-variables.css",
    "copy:workers": "run-s copy:workers:mkdir copy:workers:js",
    "copy:workers:js": "cp -R src/workers/*.js dist/workers/",
    "copy:workers:mkdir": "mkdir -p dist/workers",
    "dev": "webpack serve --config webpack/development.mjs",
    "fmt": "run-p fmt:*",
    "fmt:javascript": "prettier --write scripts src tsconfig.json webpack/",
    "fmt:package-json": "prettier-package-json --write package.json",
    "lint": "eslint src scripts webpack eslint.config.ts --fix",
    "postcss": "run-p postcss:*",
    "postcss:ciphers": "test -f ./public/css/ciphers.min.css || postcss ./public/css/ciphers.css --dir ./public/css --ext .min.css",
    "postcss:contact": "test -f ./public/css/contact.min.css || postcss ./public/css/contact.css --dir ./public/css --ext .min.css",
    "postcss:error": "test -f ./public/css/error.min.css || postcss ./public/css/error.css --dir ./public/css --ext .min.css",
    "postcss:post": "test -f ./public/css/post.min.css ] || postcss ./public/css/post.css --dir ./public/css --ext .min.css",
    "postcss:styles": "test -f ./public/css/styles.min.css ] || postcss ./public/css/styles.css --dir ./public/css --ext .min.css",
    "postcss:themes": "test -f ./public/css/sb.min.css ] || postcss ./public/css/themes/sb.css --dir ./public/css/themes --ext .min.css",
    "precommit": "lint-staged",
    "prerender": "DEBUG=\"arc:prerenderer:*\" bun run scripts/prerender.tsx",
    "sentry:update-source-maps": "bunx @sentry/wizard@latest -i sourcemaps",
    "serve": "serve -p 4200 dist/",
    "serve:void-theme": "serve -p 3000 public/css/themes/void",
    "start": "bun run serve",
    "storybook": "NODE_ENV=development storybook dev -p 6006",
    "sync:void-css": "run-s clean:void-css copy:void-css",
    "test": "bun test"
  },
  "dependencies": {
    "@arcjr/config": "workspace:*",
    "@arcjr/types": "workspace:*",
    "@arcjr/void-components": "workspace:*",
    "@arcjr/void-css": "workspace:*",
    "@arcjr/void-tokens": "workspace:*",
    "@sentry/browser": "^10.27.0",
    "@sentry/bun": "^10.27.0",
    "@sentry/core": "^10.27.0",
    "@sentry/react": "^10.21.0",
    "@sentry/webpack-plugin": "^4.5.0",
    "@tanstack/react-query": "^5.87.1",
    "classnames": "^2.5.1",
    "gsap": "^3.12.7",
    "highlight.js": "^11.11.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-markdown": "^10.1.0",
    "react-router": "^7.8.2",
    "react-router-dom": "^7.8.2",
    "react-syntax-highlighter": "^15.6.6",
    "remark-gfm": "^4.0.1",
    "sleepydogs": "1.0.8-alpha.4"
  },
  "peerDependencies": {
    "typescript": "latest"
  },
  "devDependencies": {
    "@eslint/css": "^0.11.0",
    "@eslint/js": "^9.35.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.2.0",
    "@happy-dom/global-registrator": "^18.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-webpack5-compiler-swc": "^4.0.1",
    "@storybook/react-webpack5": "^9.1.5",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.2.12",
    "@types/debug": "^4.1.12",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.10.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "autoprefixer": "^10.4.21",
    "commander": "^13.1.0",
    "cssnano": "^7.0.6",
    "debug": "^4.4.0",
    "dompurify": "^3.2.5",
    "dotenv": "^17.2.3",
    "eslint": "^9.35.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^9.1.5",
    "find-up": "5",
    "front-matter": "^4.0.2",
    "globals": "^16.4.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "jiti": "^2.5.1",
    "jsdom": "^27.0.0",
    "json-loader": "^0.5.7",
    "lint-staged": "^15.5.0",
    "marked": "^15.0.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "prettier": "^3.5.3",
    "prettier-package-json": "^2.8.0",
    "process": "^0.11.10",
    "react-refresh": "^0.17.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "serve": "^14.2.4",
    "storybook": "^9.1.5",
    "supports-color": "^10.2.2",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.14",
    "thread-loader": "^4.0.4",
    "typescript-eslint": "^8.43.0",
    "webpack": "^5.99.5",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1",
    "webpack-merge": "^6.0.1"
  },
  "engines": {
    "bun": ">=1.3.2",
    "node": ">=24",
    "npm": ">=10"
  },
  "lint-staged": {
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  },
  "packageManager": "bun@1.3.2"
}
